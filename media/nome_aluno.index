                        <label for="cidade_nascimento">Cidade:</label>
                        <input type="text" id="cidade_nascimento" name="cidade_nascimento" required>
                    </div>
                    
                    <div class="form-group">
                        <label>SEXO:</label>
                        <div class="radio-group">
                            <label><input type="radio" name="sexo" value="feminino" required> Feminino</label>
