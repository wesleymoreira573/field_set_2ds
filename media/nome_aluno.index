                        <input type="number" id="carga_horaria" name="carga_horaria" min="0">
                    </div>
                </fieldset>
                
                <!-- INSTITUIÇÃO/CURSO -->
                <fieldset>
                    <legend>INSTITUIÇÃO/CURSO</legend>
                    <div class="form-group">
                        <label for="instituicao_curso">Instituição:</label>
                        <input type="text" id="instituicao_curso" name="instituicao_curso">
                    </div>
                    
                    <div class="form-group">
                        <label for="nome_curso">Nome do Curso:</label>
                        <input type="text" id="nome_curso" name="nome_curso">
                    </div>
                    
                    <div class="form-group">
                        <label>Status do Curso:</label>
                        <div class="radio-group">
                            <label><input type="radio" name="status_curso" value="em_andamento" required> Em andamento</label>
                            <label><input type="radio" name="status_curso" value="titulo_obtido"> Com título obtido</label>
                        </div>
                    </div>
                    
                    <div class="row">
                        <div class="col">
                            <div class="form-group">
                                <label for="inicio_curso">Início:</label>
                                <input type="number" id="inicio_curso" name="inicio_curso" min="1900" max="2100">
                            </div>
                        </div>
                        <div class="col">
                            <div class="form-group">
                                <label for="conclusao_curso">Conclusão:</label>
                                <input type="number" id="conclusao_curso" name="conclusao_curso" min="1900" max="2100">
                            </div>
                        </div>
                        <div class="col">
                            <div class="form-group">
                                <label for="obtencao_titulo">Obtenção do título:</label>
                                <input type="number" id="obtencao_titulo" name="obtencao_titulo" min="1900" max="2100">
                            </div>
                        </div>
                    </div>
                    
                    <div class="form-group">
                        <label for="titulo_monografia">Título da Monografia/Dissertação/Tese:</label>
                        <textarea id="titulo_monografia" name="titulo_monografia" rows="3"></textarea>
                    </div>
                    
                    <div class="form-group">
                        <label for="nome_orientador">Nome Completo do Orientador:</label>
                        <input type="text" id="nome_orientador" name="nome_orientador">
                    </div>
                    
                    <div class="form-group">
                        <label>Bolsa:</label>
                        <div class="radio-group">
                            <label><input type="radio" name="bolsa" value="sim" required> Sim</label>
                            <label><input type="radio" name="bolsa" value="nao"> Não</label>
                        </div>
                    </div>
                    
                    <div class="form-group">
                        <label for="entidade_financeira">Entidade Financeira:</label>
                        <input type="text" id="entidade_financeira" name="entidade_financeira">
                    </div>
                </fieldset>
            </fieldset>
            
            <!-- ATUAÇÃO PROFISSIONAL -->
            <fieldset>
                <legend>ATUAÇÃO PROFISSIONAL</legend>
                
                <!-- INSTITUIÇÃO DE TRABALHO -->
                <fieldset>
                    <legend>INSTITUIÇÃO DE TRABALHO</legend>
                    <div class="form-group">
                        <label for="instituicao_trabalho">Instituição/Empresa:</label>
                        <input type="text" id="instituicao_trabalho" name="instituicao_trabalho">
                    </div>
                    
                    <div class="form-group">
                        <label for="sigla_instituicao">Sigla:</label>
                        <input type="text" id="sigla_instituicao" name="sigla_instituicao">
                    </div>
                    
                    <div class="form-group">
                        <label>ATIVIDADE(S) EXERCIDA(S) NA INSTITUIÇÃO:</label>
                        <div class="checkbox-group">
                            <label><input type="checkbox" name="atividades[]" value="conselhos"> Conselhos, comissões e consultoria</label>
                            <label><input type="checkbox" name="atividades[]" value="direcao"> Direção e administração</label>
                            <label><input type="checkbox" name="atividades[]" value="ensino"> Ensino</label>
                            <label><input type="checkbox" name="atividades[]" value="estagio"> Estágio</label>
                            <label><input type="checkbox" name="atividades[]" value="extensao"> Extensão universitária</label>
                            <label><input type="checkbox" name="atividades[]" value="pesquisa"> Pesquisa e desenvolvimento</label>
                            <label><input type="checkbox" name="atividades[]" value="servicos"> Serviços técnicos e especializados</label>
                            <label><input type="checkbox" name="atividades[]" value="treinamentos"> Treinamentos ministrados</label>
                            <label><input type="checkbox" name="atividades[]" value="outras"> Outras atividades técnico-científicas</label>
                        </div>
                    </div>
                </fieldset>
                
                <!-- VÍNCULO INSTITUCIONAL -->
                <fieldset>
                    <legend>VÍNCULO INSTITUCIONAL</legend>
                    <div class="form-group">
                        <label>Período:</label>
                        <div class="radio-group">
                            <label><input type="radio" name="periodo_vinculo" value="anterior" required> Anterior</label>
                            <label><input type="radio" name="periodo_vinculo" value="atual"> Atual</label>
                        </div>
                    </div>
                    
                    <div class="row">
                        <div class="col">
                            <div class="form-group">
                                <label for="inicio_mes">Início - Mês:</label>
                                <select id="inicio_mes" name="inicio_mes">
                                    <option value="">Selecione</option>
                                    <option value="01">Janeiro</option>
                                    <option value="02">Fevereiro</option>
                                    <option value="03">Março</option>
                                    <option value="04">Abril</option>
                                    <option value="05">Maio</option>
                                    <option value="06">Junho</option>
                                    <option value="07">Julho</option>
                                    <option value="08">Agosto</option>
                                    <option value="09">Setembro</option>
                                    <option value="10">Outubro</option>
                                    <option value="11">Novembro</option>
                                    <option value="12">Dezembro</option>
                                </select>
                            </div>
                        </div>
                        <div class="col">
                            <div class="form-group">
                                <label for="inicio_ano">Início - Ano:</label>
                                <input type="number" id="inicio_ano" name="inicio_ano" min="1900" max="2100">
                            </div>
                        </div>
                    </div>
                    
                    <div class="row">
                        <div class="col">
                            <div class="form-group">
                                <label for="fim_mes">Fim - Mês:</label>
                                <select id="fim_mes" name="fim_mes">
                                    <option value="">Selecione</option>
                                    <option value="01">Janeiro</option>
                                    <option value="02">Fevereiro</option>
                                    <option value="03">Março</option>
                                    <option value="04">Abril</option>
                                    <option value="05">Maio</option>
                                    <option value="06">Junho</option>
                                    <option value="07">Julho</option>
                                    <option value="08">Agosto</option>
                                    <option value="09">Setembro</option>
                                    <option value="10">Outubro</option>
                                    <option value="11">Novembro</option>
                                    <option value="12">Dezembro</option>
                                </select>
                            </div>
                        </div>
                        <div class="col">
                            <div class="form-group">
                                <label for="fim_ano">Fim - Ano:</label>
                                <input type="number" id="fim_ano" name="fim_ano" min="1900" max="2100">
                            </div>
                        </div>
                    </div>
                    
                    <div class="form-group">
                        <label for="tipo_vinculo">Tipo de Vínculo:</label>
                        <select id="tipo_vinculo" name="tipo_vinculo">
                            <option value="">Selecione</option>
                            <option value="servidor_publico">Servidor Público</option>
                            <option value="celetista">Celetista</option>
                            <option value="outro">Outro</option>
                        </select>
                    </div>
                    
                    <div class="form-group">
                        <label for="carga_horaria_semanal">Carga Horária Semanal:</label>
                        <input type="number" id="carga_horaria_semanal" name="carga_horaria_semanal" min="0" max="168">
                    </div>
                </fieldset>
            </fieldset>
            
            <button type="submit" class="submit-btn">Enviar Formulário</button>
        </form>
    </div>
</body>
</html> 